function [c,ceq] = econ2(x) 

global e;
c(1) = x(1)^2 + x(2)^2 - 225 ; 
c(2) = x(1) - 3*x(2) + 10 ; 
c(3) = 9*x(1) 

ceq = []; 
end


function [c,ceq] = econ1(x) 

global e;
c(1) = 1 + 0.1*cos(16*atan(x(1)/x(2))) - x(1)^2 - x(2)^2 ; 
c(2) = (x(1) - 0.5)^2 + (x(2) - 0.5)^2 - 0.5 ;
c(3) = x(2) - e;

ceq = []; 
end