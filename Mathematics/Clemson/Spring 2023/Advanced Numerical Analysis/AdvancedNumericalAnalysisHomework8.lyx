\begin_layout Subsection*
QR Post Process
\end_layout

\begin_layout Standard
\align center
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

function [Q,R] = QRpostprocess(Q,R)
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

[m,n] = size(Q);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

for i = 1:n   
\end_layout

\begin_layout Plain Layout

   if R(i,i) < 0 
\end_layout

\begin_layout Plain Layout

      R(i,:) = -R(i,:);   
\end_layout

\begin_layout Plain Layout

      Q(:,i) = -Q(:,i);    
\end_layout

\begin_layout Plain Layout

   end;
\end_layout

\begin_layout Plain Layout

end;
\end_layout

\end_inset


\end_layout

\begin_layout Subsection*
QR Algorithm
\end_layout

\begin_layout Standard
\align center
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

function [Qk,Rk,Ak] = QRalgorithm(A,k)
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

[m,n] = size(A); Ak = A; Rk = eye(n); Qk = eye(n);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

for i = 1:k    
\end_layout

\begin_layout Plain Layout

   [Q,R] = qr(Ak); 
\end_layout

\begin_layout Plain Layout

   [Q,R] = QRpostprocess(Q,R);
\end_layout

\begin_layout Plain Layout

   Ak = R*Q;    
\end_layout

\begin_layout Plain Layout

   Qk = Qk*Q;    
\end_layout

\begin_layout Plain Layout

   Rk = R*Rk; 
\end_layout

\begin_layout Plain Layout

end;
\end_layout

\end_inset


\end_layout

\begin_layout Subsection*
Simultaneous Iteration
\end_layout

\begin_layout Standard
\align center
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

function [Qk,Rk,Ak] = SimultaneousIteration(A,k)
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

[m,n] = size(A); Qk = eye(n); Rk = eye(n);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

for i = 1:k   
\end_layout

\begin_layout Plain Layout

   Z = A*Qk;    
\end_layout

\begin_layout Plain Layout

   [Qk,R] = qr(Z); 
\end_layout

\begin_layout Plain Layout

   [Qk,R] = QRpostprocess(Qk,R);  
\end_layout

\begin_layout Plain Layout

   Ak = Qk'*A*Qk;   
\end_layout

\begin_layout Plain Layout

   Rk = R*Rk;
\end_layout

\begin_layout Plain Layout

end;
\end_layout

