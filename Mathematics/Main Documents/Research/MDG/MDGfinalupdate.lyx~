ring A=(0,x,y,z,u,v),(e1,e2,e3,e4,e5,e6,
\end_layout

\begin_layout Plain Layout

e12,e13,e14,e23,e24,e26,e35,e45,e56,
\end_layout

\begin_layout Plain Layout

e123,e124,e1345,e2345,e2456,e12345),Wp(V);
\end_layout

