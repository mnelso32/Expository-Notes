\end_layout

\begin_layout Plain Layout

poly f(2)(6) = e2*e6 - y*e26; 
\end_layout

\begin_layout Plain Layout

poly f(3)(4) = e3*e4 - v*e35 + v*e45;   
\end_layout

\begin_layout Plain Layout

poly f(3)(5) = e3*e5 - u*e35;   
\end_layout

\begin_layout Plain Layout

