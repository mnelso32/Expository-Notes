\begin_layout Plain Layout

 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
node[circle, fill=black, inner sep=1pt, label=left:$x^2 $] (e1) at (0,0)
 {}; 
\end_layout

\begin_layout Plain Layout


