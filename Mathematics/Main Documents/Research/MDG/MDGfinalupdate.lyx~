\end_layout

\begin_layout Plain Layout

poly f(4)(5) = e4*e5 - x*e45;   
\end_layout

\begin_layout Plain Layout

poly f(4)(6) = e4*e6 + zu*e24 - x*e26; 
\end_layout

\begin_layout Plain Layout

poly f(5)(6) = e5*e6 - u*e56;     
\end_layout

\begin_layout Plain Layout

poly f(1)(23) = e1*e23 - v*e123;   
\end_layout

\begin_layout Plain Layout

poly f(1)(24) = e1*e24 - v*e124;  
\end_layout

\begin_layout Plain Layout

