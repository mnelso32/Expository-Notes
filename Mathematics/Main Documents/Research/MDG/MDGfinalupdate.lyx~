\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{center}
\backslash
begin{tikzcd}[column sep=50, row sep =50]
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

 S 
\backslash
arrow[r, 
\begin_inset Quotes eld
\end_inset

 
\backslash
pi  
\begin_inset Quotes eld
\end_inset

, two heads] 
\backslash
arrow[dr, 
\begin_inset Quotes eld
\end_inset

 
\backslash
