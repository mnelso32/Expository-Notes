\begin_inset Formula 
\[
\mathrm{J}_{\boldsymbol{f}}=\begin{pmatrix}-2x & -y & z\\
w & -x & -2y\\
0 & w & x\\
y & z & 0
\end{pmatrix}.
\]

\end_inset

