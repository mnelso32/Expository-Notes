</lyxtabular>

\end_inset


\end_layout

\begin_layout Standard
If the condition on the RHS of the table above does not hold, then we set
 the corresponding polynomial to 
\begin_inset Formula $0$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
hfill
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\paragraph_spacing single
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

Next we define
\begin_inset Formula 
\begin{align*}
\mathrm{P}=\mathrm{P}(\varepsilon,\delta) & =\Bbbk[\boldsymbol{L},\boldsymbol{M}]\slash\langle\boldsymbol{P}(\varepsilon),\boldsymbol{\Lambda}(\delta),\boldsymbol{\Delta}(\delta),\boldsymbol{\Gamma}(\delta)\rangle\\
\mathrm{H}=\mathrm{H}(\varepsilon,\delta) & =\Bbbk[\boldsymbol{L},\boldsymbol{M}]\slash\langle\boldsymbol{H}(\varepsilon),\boldsymbol{\Lambda}(\delta),\boldsymbol{\Delta}(\delta)\rangle,\\
\end{align*}

\end_inset

where 
\begin_inset Formula $\varepsilon,\delta\in\mathbb{N}\cup\{\infty\}$
\end_inset

 (typically we take 
\begin_inset Formula $\delta=\infty$
\end_inset

).
 
\begin_inset Formula 
\[
A(\varepsilon)=\Bbbk[\boldsymbol{L},\boldsymbol{M}]\slash\langle\rangle
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A_{\varepsilon}=\Bbbk[\boldsymbol{M}]\slash\langle\boldsymbol{G},\boldsymbol{\Gamma},\boldsymbol{\Lambda},\boldsymbol{H}_{\varepsilon}\rangle$
\end_inset

 and we set 
\begin_inset Formula $X_{\varepsilon}=\mathrm{Spec}\,A_{\varepsilon}$
\end_inset

.
 Note that if 
\begin_inset Formula $|e_{i_{1}}|<|e_{i_{3}}|$
\end_inset

, then
\begin_inset Formula 
\begin{align*}
H_{\boldsymbol{i}}^{k} & =\sum\limits _{\boldsymbol{j}}(M_{i_{1},i_{2}}^{j_{1}}L_{i_{3}}^{j_{2}}-(-1)^{|e_{j_{1}}||e_{j_{2}}|}L_{i_{1}}^{j_{2}}M_{i_{2},i_{3}}^{j_{1}})M_{j_{1},j_{2}}^{k}=\sum\limits _{\substack{\boldsymbol{j}}
}M_{i_{1},i_{2}}^{j_{1}}L_{i_{3}}^{j_{2}}M_{j_{1},j_{2}}^{k}.
\end{align*}

\end_inset

Similarly, if 
\begin_inset Formula $|e_{i_{1}}|<|e_{i_{3}}|$
\end_inset

, then
\begin_inset Formula 
\[
P_{\boldsymbol{i}}^{k}(\varepsilon)=\sum\limits _{\boldsymbol{j}}M_{i_{1},i_{2}}^{j_{1}}L_{i_{3}}^{j_{2}}L_{i_{4}}^{j_{3}}L_{j_{1}}^{j_{5}}M_{j_{2},j_{3}}^{j_{4}}M_{j_{5},j_{4}}^{k}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{theorem}
\backslash
label{theorem}
\end_layout

\end_inset

 We have the following bijection of sets:
\begin_inset Formula 
\[
\left\{ \mathrm{GL}_{n}(\Bbbk)\text{-orbits of }\mathrm{Hom}_{\Bbbk\text{-alg}}(A_{\varepsilon},R)\right\} \longleftrightarrow\left\{ \begin{array}{c}
\text{isomorphism classes of \ensuremath{\varepsilon}-associative multiplications on }F\end{array}\right\} .
\]

\end_inset

Thus the 
\begin_inset Formula $\mathrm{GL}_{n}(\Bbbk)$
\end_inset

-orbits of the 
\begin_inset Formula $\Bbbk$
\end_inset

-valued points of 
\begin_inset Formula $X_{\varepsilon}$
\end_inset

 are in bijection 
\begin_inset Formula $[\mathrm{Mult}_{\varepsilon}(F)]:=\mathrm{Mult}_{\varepsilon}(F)\slash\sim$
\end_inset

 where 
\begin_inset Formula $\mathrm{Mult}_{\varepsilon}(F)$
\end_inset

 denotes the set of all 
\begin_inset Formula $\varepsilon$
\end_inset

-associative multiplications on 
\begin_inset Formula $F$
\end_inset

 and where 
\begin_inset Formula $\sim$
\end_inset

 is the isomorphism equivalence relation.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{theorem}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

Now suppose that 
\begin_inset Formula $R$
\end_inset

 is a 
\begin_inset Formula $\Bbbk$
\end_inset

-algebra.
 Then 
\begin_inset Formula $G:=R\otimes_{\Bbbk}F$
\end_inset

 is a finite free graded 
\begin_inset Formula $R$
\end_inset

-module with 
\begin_inset Formula $G_{0}\simeq R$
\end_inset

, 
\begin_inset Formula $G_{i}=0$
\end_inset

 for all 
\begin_inset Formula $i<0$
\end_inset

, and 
\begin_inset Formula $G_{+}\neq0$
\end_inset

.
 We set
\begin_inset Formula 
\[
\mathrm{A}_{R}^{\mathrm{d}}(G):=\mathrm{A}_{\Bbbk}^{\mathrm{d}}(F)\otimes_{\Bbbk}R\simeq R[\boldsymbol{D}]\slash\langle\boldsymbol{\Delta}\rangle\cup\langle\{D_{j}^{i}\mid|e_{i}|\neq|e_{j}|-1\}\rangle.
\]

\end_inset

It is clear that we have an inclusion of sets 
\begin_inset Formula $h_{\mathrm{A}_{\Bbbk}^{\mathrm{d}}(F)}(R)\subseteq h_{\mathrm{A}_{R}^{\mathrm{d}}(G)}(R).$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{prop}
\backslash
label{prop}
\end_layout

\end_inset

 Let 
\begin_inset Formula $G=\mathrm{Aut}(R\slash\Bbbk)$
\end_inset

.
 Then 
\begin_inset Formula $G$
\end_inset

 acts on 
\begin_inset Formula $h_{\mathrm{A}_{R}^{\mathrm{d}}(G)}(R)$
\end_inset

 and the set of all fixed points is precisely 
\begin_inset Formula $h_{\mathrm{A}_{\Bbbk}^{\mathrm{d}}(F)}(R)$
\end_inset

.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{prop}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
We set
\begin_inset Formula 
\[
\mathrm{A}^{(n)}=\mathrm{A}=\mathbb{Z}[\boldsymbol{M},\boldsymbol{L},\boldsymbol{D}]\slash\langle\boldsymbol{G},\Gamma,\boldsymbol{\Lambda},\text{\ensuremath{\Theta}},\boldsymbol{H},\boldsymbol{\Pi}\rangle
\]

\end_inset

where 
\begin_inset Formula $F_{+}=\mathbb{Z}^{n}$
\end_inset

.
 Then we have
\begin_inset Formula 
\[
\mathrm{A}\otimes_{\mathbb{Z}}R=R[\boldsymbol{M},\boldsymbol{L},\boldsymbol{D}]\slash\langle\boldsymbol{G},\Gamma,\boldsymbol{\Lambda},\text{\ensuremath{\Theta}},\boldsymbol{H},\boldsymbol{\Pi}\rangle
\]

\end_inset

which classifies MLDG structures on 
\begin_inset Formula $F\otimes_{\mathbb{Z}}R=R^{n}$
\end_inset

.
 Similarly, 
\begin_inset Formula 
\[
\mathrm{A}\otimes_{\mathbb{Z}}\mathbb{Q}=\mathrm{A}_{\mathbb{Q}}=\mathbb{Q}[\boldsymbol{M},\boldsymbol{L},\boldsymbol{D}]\slash\langle\boldsymbol{G},\Gamma,\boldsymbol{\Lambda},\text{\ensuremath{\Theta}},\boldsymbol{H},\boldsymbol{\Pi}\rangle
\]

\end_inset

We are interested in
\begin_inset Formula 
\[
\mathrm{h}_{\mathbb{Q}}(K):=\mathrm{Hom}_{\mathbb{Q}\text{-alg}}(\mathrm{A}_{\mathbb{Q}},K)
\]

\end_inset

where 
\begin_inset Formula $K\slash\mathbb{Q}$
\end_inset

 is a finite extension of degree 
\begin_inset Formula $n$
\end_inset

.
 Then 
\begin_inset Formula $\mathrm{h}_{\mathbb{Q}}(K)$
\end_inset

 classifies MLDG 
\begin_inset Formula $\mathbb{Q}$
\end_inset

-algebras.
\end_layout

\begin_layout Standard
Consider
\begin_inset Formula 
\[
\mathbb{Z}_{p}[x,y,z]\slash\langle x^{2},p^{2},pz,xy,y^{2}z^{2}\rangle=(\mathbb{Z}\slash p^{2})[x,y,z]\slash\langle x^{2},pz,xy,y^{2}z^{2}\rangle
\]

\end_inset

Consider
\begin_inset Formula 
\[
\mathbb{Z}_{p}[x,y,z]\slash\langle x^{2},w^{2},zw,px,p^{2}z^{2}\rangle
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Section
The Symmetric DG Algebra
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $R$
\end_inset

 be a commutative ring, let 
\begin_inset Formula $A$
\end_inset

 be a 
\begin_inset Formula $\mathbb{Z}$
\end_inset

-graded 
\begin_inset Formula $R$
\end_inset

-module such that 
\begin_inset Formula $A_{0}=R$
\end_inset

 which is also equipped with a 
\begin_inset Formula $\mathbb{Z}$
\end_inset

-linear differential 
\begin_inset Formula $\mathrm{d}\colon A\to A$
\end_inset

 giving it the structure of a chain complex.
 Note that the differential need not be 
\begin_inset Formula $R$
\end_inset

-linear and note that 
\begin_inset Formula $A$
\end_inset

 may be nonzero in negative homological degree.
 In this section, we will construct the symmetric DG algebra of 
\begin_inset Formula $A$
\end_inset

, which we denote by 
\begin_inset Formula $\mathrm{S}(A)$
\end_inset

.
 After constructing the symmetric DG algebra in this general setting, we
 then specialize to the case we are mostly interesting in, namely that 
\begin_inset Formula $A$
\end_inset

 is an 
\begin_inset Formula $R$
\end_inset

-complex centered at 
\begin_inset Formula $R$
\end_inset

 meaning the differential of 
\begin_inset Formula $A$
\end_inset

 is 
\begin_inset Formula $R$
\end_inset

-linear with 
\begin_inset Formula $A_{0}=R$
\end_inset

 and 
\begin_inset Formula $A_{<0}=0$
\end_inset

.
 In this case, we sometimes denote the symmetric DG algebra of 
\begin_inset Formula $A$
\end_inset

 by 
\begin_inset Formula $\mathrm{S}_{R}(A)$
\end_inset

 with 
\begin_inset Formula $R$
\end_inset

 in the subscript in order to emphasize that 
\begin_inset Formula $A$
\end_inset

 is centered at 
\begin_inset Formula $R$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

Before we give a rigorous construction of the symmetric DG algebra, we wish
 to help motivate the reader by giving an informal description of it in
 this special case where 
\begin_inset Formula $A$
\end_inset

 is an 
\begin_inset Formula $R$
\end_inset

-complex centered at 
\begin_inset Formula $R$
\end_inset

.
 In this case, the underlying graded algebra of 
\begin_inset Formula $S=\mathrm{S}_{R}(A)$
\end_inset

 is the usual symmetric 
\begin_inset Formula $R$
\end_inset

-algebra 
\begin_inset Formula $\mathrm{Sym}(A_{+})$
\end_inset

 where we view 
\begin_inset Formula $A_{+}$
\end_inset

 as just an 
\begin_inset Formula $R$
\end_inset

-module.
 However 
\begin_inset Formula $S$
\end_inset

 obtains a bi-graded structure using homological degree and total degree:
 we have a decomposition of 
\begin_inset Formula $S$
\end_inset

 into 
\begin_inset Formula $R$
\end_inset

-modules:
\begin_inset Formula 
\[
S=\bigoplus_{i\geq0}S_{i}=\bigoplus_{m\geq0}S^{m}=\bigoplus_{i,m\geq0}S_{i}^{m}.
\]

\end_inset

We refer to the 
\begin_inset Formula $i$
\end_inset

 in the subscript as homological degree
\series bold
 
\series default
and we refer to the 
\begin_inset Formula $m$
\end_inset

 in the superscript as total degree.
 We have 
\begin_inset Formula $S_{0}=S^{0}=S_{0}^{0}=R$
\end_inset

 and 
\begin_inset Formula $S^{1}=A_{+}$
\end_inset

.
 More generally, for 
\begin_inset Formula $i,m\geq1$
\end_inset

, the 
\begin_inset Formula $R$
\end_inset

-module 
\begin_inset Formula $S_{i}^{m}$
\end_inset

 is the 
\begin_inset Formula $R$
\end_inset

-span of all homogeneous elementary products of the form 
\begin_inset Formula $\boldsymbol{a}=a_{1}\cdots a_{m}$
\end_inset

 where 
\begin_inset Formula $a_{1},\dots,a_{m}\in A_{+}$
\end_inset

 are homogeneous (with respect to homological degree of course) such that
\begin_inset Formula 
\[
|\boldsymbol{a}|=|a_{1}|+\cdots+|a_{m}|=i.
\]

\end_inset

In particular, note that 
\begin_inset Formula $A=S^{\leq1}=R+A_{+}$
\end_inset

, thus we view 
\begin_inset Formula $A$
\end_inset

 as being the total degree 
\begin_inset Formula $\leq1$
\end_inset

 part of 
\begin_inset Formula $S$
\end_inset

.
 The differential of 
\begin_inset Formula $A$
\end_inset

 extends the differential of 
\begin_inset Formula $S$
\end_inset

 in a natural way and is defined on homogeneous elementary products 
\begin_inset Formula $\boldsymbol{a}=a_{1}\cdots a_{m}$
\end_inset

 by
\begin_inset Formula 
\begin{align}
\mathrm{d}\boldsymbol{a} & =\sum_{j=1}^{m}(-1)^{|a_{1}|+\cdots+|a_{j-1}|}a_{1}\cdots\mathrm{d}(a_{j})\cdots a_{m}.\label{eq:caretaken}
\end{align}

\end_inset

If each of the 
\begin_inset Formula $a_{j}$
\end_inset

 in 
\begin_inset ERT
status open

\begin_layout Plain Layout

(
\backslash
ref{eq:caretaken})
\end_layout

\end_inset

 live in homological degree 
\begin_inset Formula $\ge2$
\end_inset

, then 
\begin_inset Formula $\mathrm{d}\boldsymbol{a}$
\end_inset

 and 
\begin_inset Formula $\boldsymbol{a}$
\end_inset

 has the same total degree, namely 
\begin_inset Formula $\deg(\mathrm{d}\boldsymbol{a})=m=\deg\boldsymbol{a}$
\end_inset

.
 However if one of the 
\begin_inset Formula $a_{j}$
\end_inset

 in 
\begin_inset ERT
status open

\begin_layout Plain Layout

(
\backslash
ref{eq:caretaken})
\end_layout

\end_inset

 lives in homological degree 
\begin_inset Formula $1$
\end_inset

, then 
\begin_inset Formula $\deg(\mathrm{d}\boldsymbol{a})=m-1$
\end_inset

.
 The diagram below illustrates how the differential acts on the bi-graded
 components: 
\begin_inset Branch tikzcd
inverted 0
status collapsed

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{center}
\backslash
begin{tikzcd}[column sep = 7] 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

A_i 
\backslash
arrow[dr] && 
\backslash
cdots 
\backslash
arrow[dl] 
\backslash
arrow[dr] && S_i ^{m-1} 
\backslash
arrow[dl] 
\backslash
arrow[dr] && S_i ^{m}  
\backslash
arrow[dl] 
\backslash
arrow[dr] &&  S_i ^{m+1} 
\backslash
arrow[dl] 
\backslash
arrow[dr] && 
\backslash
cdots 
\backslash
arrow[dl] 
\backslash
arrow[dr] && 
\backslash
mathbb{K} _i 
\backslash
arrow[dl]
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash

\backslash

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

& A_{i-1} && S_{i-1} ^{m-2}   && S_{i-1} ^{m-1}  && S_{i-1} ^{m} && S_{i-1}
 ^{m+1} && 
\backslash
mathbb{K} _{i-1}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
end{tikzcd}
\backslash
end{center}
\end_layout

\end_inset


\end_layout

\end_inset

where we set 
\begin_inset Formula $\mathbb{K}$
\end_inset

 to be the koszul DG algebra induced by 
\begin_inset Formula $\mathrm{d}\colon A_{1}\to A_{0}$
\end_inset

.
 Thus the differential of 
\begin_inset Formula $S$
\end_inset

 connects the usual differential of 
\begin_inset Formula $A$
\end_inset

 on the far left to a koszul differential on the far right.
 In order to keep track of how the differential operates on the bi-graded
 components, we express 
\begin_inset Formula $\mathrm{d}$
\end_inset

 as
\begin_inset Formula 
\[
\mathrm{d}=\eth+\partial,
\]

\end_inset

where 
\begin_inset Formula $\eth$
\end_inset

 is the component of 
\begin_inset Formula $\mathrm{d}$
\end_inset

 which respects total degree and where 
\begin_inset Formula $\partial$
\end_inset

 is the component of 
\begin_inset Formula $\mathrm{d}$
\end_inset

 which drops total degree by 
\begin_inset Formula $1$
\end_inset

.
 In the next example, we consider a free resolution of a cyclic module and
 work out what the symmetric DG algebra looks like in this case.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{example}
\backslash
label{symmetricdgexample}
\end_layout

\end_inset

 Let 
\begin_inset Formula $R=\Bbbk[x,y]$
\end_inset

, let 
\begin_inset Formula $I=\langle x^{2},xy\rangle$
\end_inset

, and let 
\begin_inset Formula $F$
\end_inset

 be Taylor resolution of 
\begin_inset Formula $\overline{R}=R\slash I$
\end_inset

.
 Let's write down the homogeneous components of 
\begin_inset Formula $F$
\end_inset

 as a graded 
\begin_inset Formula $R$
\end_inset

-module as well as how the differential acts on the homogeneous basis:
\begin_inset Formula 
\begin{align*}
F_{0} & =R & \mathrm{d}e_{1} & =x^{2}\\
F_{1} & =Re_{1}+Re_{2} & \mathrm{d}e_{2} & =xy\\
F_{2} & =Re_{12}, & \mathrm{d}e_{12} & =xe_{2}-ye_{1},
\end{align*}

\end_inset

Note that the Taylor resolution usually comes equipped with a multiplication
 called the Taylor multiplication.
 Let us denote this by 
\begin_inset Formula $\star$
\end_inset

 so as not to confuse it with the multiplication 
\begin_inset Formula $\cdot$
\end_inset

 of 
\begin_inset Formula $S=\mathrm{S}_{R}(F)$
\end_inset

.
 Now let's write down the homogeneous components of 
\begin_inset Formula $S$
\end_inset

 as a graded 
\begin_inset Formula $R$
\end_inset

-module (with respect to homological degree): we have
\begin_inset Formula 
\begin{align*}
S_{0} & =R\\
S_{1} & =Re_{1}+Re_{2}\\
S_{2} & =Re_{12}+Re_{1}e_{2}\\
S_{3} & =Re_{1}e_{12}+Re_{2}e_{12}\\
S_{4} & =Re_{12}^{2}+Re_{1}e_{2}e_{12}\\
 & \vdots\\
S_{2k-1} & =Re_{1}e_{12}^{k-1}+Re_{2}^{k-1}\\
S_{2k} & =Re_{12}^{k}+Re_{1}e_{2}e_{12}^{k-1}\\
S_{2k+1} & =Re_{1}e_{12}^{k}+Re_{2}e_{12}^{k}\\
 & \vdots
\end{align*}

\end_inset

Note that
\begin_inset Formula 
\begin{align*}
\mathrm{d}(e_{1}e_{2}-xe_{12}) & =\mathrm{d}(e_{1}e_{2})-x\mathrm{d}(e_{12})\\
 & =\mathrm{d}(e_{1})e_{2}-e_{1}\mathrm{d}(e_{2})-x(xe_{2}-ye_{1})\\
 & =x^{2}e_{2}-xye_{1}-x^{2}e_{2}+xye_{1}\\
 & =0.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{example}
\end_layout

\end_inset


\end_layout

\begin_layout Subsection
Construction of the Symmetric DG Algebra of 
\begin_inset Formula $A$
\end_inset


\end_layout

\begin_layout Standard
We now provide a rigorous construction of 
\begin_inset Formula $\mathrm{S}(A)$
\end_inset

 in the general case where the differential of 
\begin_inset Formula $A$
\end_inset

 need not be 
\begin_inset Formula $R$
\end_inset

-linear and where 
\begin_inset Formula $A_{<0}$
\end_inset

 is not necessarily zero.
 Our construction will occur in three steps:
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
hfill
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\series bold
Step 1: 
\series default
We define the 
\series bold
non-unital
\series default
 
\series bold
tensor DG algebra
\series default
 of 
\begin_inset Formula $A$
\end_inset

 to be
\begin_inset Formula 
\[
\mathrm{U}_{\mathbb{Z}}(A):=\bigoplus_{n=1}^{\infty}A^{\otimes n},
\]

\end_inset

where the tensor product is taken as 
\begin_inset Formula $\mathbb{Z}$
\end_inset

-complexes.
 An elementary tensor in 
\begin_inset Formula $U=\mathrm{U}_{\mathbb{Z}}(A)$
\end_inset

 is denoted 
\begin_inset Formula $\boldsymbol{a}=a_{1}\otimes\cdots\otimes a_{n}$
\end_inset

 where 
\begin_inset Formula $a_{1},\dots,a_{n}\in A$
\end_inset

 and 
\begin_inset Formula $n\ge1$
\end_inset

.
 The differential of 
\begin_inset Formula $U$
\end_inset

 is denoted by 
\begin_inset Formula $\mathrm{d}$
\end_inset

 again to simplify notation and is defined on 
\begin_inset Formula $\boldsymbol{a}$
\end_inset

 by
\begin_inset Formula 
\[
\mathrm{d}\boldsymbol{a}=\sum_{j=1}^{n}(-1)^{|a_{1}|+\cdots+|a_{j-1}|}a_{1}\otimes\cdots\otimes\mathrm{d}a_{j}\otimes\cdots\otimes a_{n}.
\]

\end_inset


\end_layout

\begin_layout Standard
We say
\series bold
 
\begin_inset Formula $\boldsymbol{a}$
\end_inset


\series default
 is a homogeneous elementary tensors if each 
\begin_inset Formula $a_{i}$
\end_inset

 is a homogeneous element in 
\begin_inset Formula $A$
\end_inset

.
 In this case, we set
\begin_inset Formula 
\[
|\boldsymbol{a}|=\sum_{i=1}^{n}|a_{i}|\quad\text{and}\quad\deg\boldsymbol{a}=\sum_{i=1}^{n}\deg a_{i},
\]

\end_inset

where 
\begin_inset Formula $\deg$
\end_inset

 is defined on elements 
\begin_inset Formula $a\in A$
\end_inset

 by
\begin_inset Formula 
\[
\deg a=\begin{cases}
1 & \text{if }a\in A_{>0}\\
0 & \text{if }a\in R\\
-1 & \text{if }a\in A_{<0}
\end{cases}
\]

\end_inset

We call 
\begin_inset Formula $|\boldsymbol{a}|$
\end_inset

 the 
\series bold
homological degree 
\series default
of 
\begin_inset Formula $\boldsymbol{a}$
\end_inset

 and we call 
\begin_inset Formula $\deg\boldsymbol{a}$
\end_inset

 the 
\series bold
total degree 
\series default
of 
\begin_inset Formula $\boldsymbol{a}$
\end_inset

.
 With 
\begin_inset Formula $|\cdot|$
\end_inset

 and 
\begin_inset Formula $\deg$
\end_inset

 defined, we observe that 
\begin_inset Formula $U$
\end_inset

 admits a bi-graded decomposition:
\begin_inset Formula 
\[
U=\bigoplus_{i\in\mathbb{Z}}U_{i}=\bigoplus_{m\in\mathbb{Z}}U^{m}=\bigoplus_{i,m\in\mathbb{Z}}U_{i}^{m},
\]

\end_inset

where the component 
\begin_inset Formula $U_{i}^{m}$
\end_inset

 consists of all finite 
\begin_inset Formula $\mathbb{Z}$
\end_inset

-linear combinations of homogeneous elementary tensors 
\begin_inset Formula $\boldsymbol{a}\in U$
\end_inset

 such that 
\begin_inset Formula $|\boldsymbol{a}|=i$
\end_inset

 and 
\begin_inset Formula $\deg\boldsymbol{a}=m$
\end_inset

.
 We equip 
\begin_inset Formula $U$
\end_inset

 with an associative (but not commutative nor unital) bi-graded 
\begin_inset Formula $\mathbb{Z}$
\end_inset

-bilinear multiplication which is defined on homogeneous elementary tensors
 by 
\begin_inset Formula $(\boldsymbol{a},\boldsymbol{a}')\mapsto\boldsymbol{a}\otimes\boldsymbol{a}'$
\end_inset

 and is extended 
\begin_inset Formula $\mathbb{Z}$
\end_inset

-bilinearly everywhere else.
 This multiplication is easily seen to satisfy Leibniz law, however note
 that 
\begin_inset Formula $U$
\end_inset

 is not unital under this multiplication since 
\begin_inset Formula $(1,1)\mapsto1\otimes1\neq1$
\end_inset

 (hence why we call this the 
\emph on
non-unital 
\emph default
tensor DG algebra).
 Also note that 
\begin_inset Formula $U$
\end_inset

 already comes equipped with an 
\begin_inset Formula $R$
\end_inset

‚Äìscalar multiplication (from the 
\begin_inset Formula $R$
\end_inset

-module structure on 
\begin_inset Formula $A$
\end_inset

), denoted 
\begin_inset Formula $(r,\boldsymbol{a})\mapsto r\boldsymbol{a}$
\end_inset

, however the multiplication of 
\begin_inset Formula $U$
\end_inset

 only agrees with the 
\begin_inset Formula $R$
\end_inset

-scalar multiplication wherever they are both defined and vanish.
 To rectify this, let 
\begin_inset Formula $\mathfrak{u}=\mathfrak{u}(A)$
\end_inset

 be the 
\begin_inset Formula $U$
\end_inset

-ideal by all elements of the form
\begin_inset Formula 
\begin{align*}
[r,a]_{\mu} & =r\otimes a-ra & [a,r]_{\mu} & =a\otimes r-ar\\{}
[r,a]_{\mathrm{d}} & =\mathrm{d}r\otimes a-\mathrm{d}(ra)+r(\mathrm{d}a) & [a,r]_{\mathrm{d}} & =(-1)^{|a|}a\otimes\mathrm{d}r-\mathrm{d}(ar)+(\mathrm{d}a)r
\end{align*}

\end_inset

where 
\begin_inset Formula $r\in R$
\end_inset

 and 
\begin_inset Formula $a\in A$
\end_inset

.
 
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{lemma}
\backslash
label{lemma}
\end_layout

\end_inset

 The differential maps 
\begin_inset Formula $\mathfrak{u}$
\end_inset

 to itself.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{lemma}
\end_layout

\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{proof}
\backslash
label{proof}
\end_layout

\end_inset

 Indeed, given 
\begin_inset Formula $r\in R$
\end_inset

 and 
\begin_inset Formula $a\in A$
\end_inset

, we have
\begin_inset Formula 
\begin{align*}
\mathrm{d}[r,a]_{\mu} & =\mathrm{d}(r\otimes a)-\mathrm{d}(ra)\\
 & =\mathrm{d}r\otimes a+r\otimes\mathrm{d}a-\mathrm{d}r\otimes a+r(\mathrm{d}a)+[r,a]_{\mathrm{d}}\\
 & =r\otimes\mathrm{d}a+r(\mathrm{d}a)+[r,a]_{\mathrm{d}}\\
 & =[r,\mathrm{d}a]_{\mu}+[r,a]_{\mathrm{d}}\\
 & \in\mathfrak{u}.
\end{align*}

\end_inset

Similarly we have
\begin_inset Formula 
\begin{align*}
\mathrm{d}[r,a]_{\mathrm{d}} & =\mathrm{d}(\mathrm{d}r\otimes a-\mathrm{d}(ra)+r(\mathrm{d}a))\\
 & =-\mathrm{d}r\otimes\mathrm{d}a+\mathrm{d}(r(\mathrm{d}a))\\
 & =-\mathrm{d}r\otimes\mathrm{d}a+\mathrm{d}(r\otimes\mathrm{d}a-[r,\mathrm{d}a]_{\mu})\\
 & =-\mathrm{d}r\otimes\mathrm{d}a+\mathrm{d}r\otimes\mathrm{d}a-\mathrm{d}[r,\mathrm{d}a]_{\mu}\\
 & =-\mathrm{d}[r,\mathrm{d}a]_{\mu}\\
 & =-[r,\mathrm{d}a]_{\mathrm{d}}\\
 & \in\mathfrak{u}.
\end{align*}

\end_inset

Similar calculations show 
\begin_inset Formula $\mathrm{d}[a,r]_{\mu}\in\mathfrak{u}$
\end_inset

 and 
\begin_inset Formula $\mathrm{d}[a,r]_{\mathrm{d}}\in\mathfrak{u}$
\end_inset

.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{proof}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
hfill
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\series bold
Step 2: 
\series default
We define the 
\series bold
tensor DG algebra 
\series default
of 
\begin_inset Formula $A$
\end_inset

 to be the quotient
\begin_inset Formula 
\[
\mathrm{T}(A):=\mathrm{U}(A)\slash\mathfrak{u}(A).
\]

\end_inset


\end_layout

\begin_layout Standard
The multiplication of 
\begin_inset Formula $U=\mathrm{U}(A)$
\end_inset

 induces a multiplication on 
\begin_inset Formula $T=\mathrm{T}(A)$
\end_inset

 which not only becomes unital but also agrees with the 
\begin_inset Formula $R$
\end_inset

-scalar multiplication on 
\begin_inset Formula $T$
\end_inset

 where they are both defined.
 Since 
\begin_inset Formula $\mathfrak{u}=\mathfrak{u}(A)$
\end_inset

 is generated by elements which are homogeneous with respect to homological
 degree and since the differential of 
\begin_inset Formula $U$
\end_inset

 maps 
\begin_inset Formula $\mathfrak{u}$
\end_inset

 to itself, it follows that the differential of 
\begin_inset Formula $U$
\end_inset

 induces a differential on 
\begin_inset Formula $T$
\end_inset

, which we again denote by 
\begin_inset Formula $\mathrm{d}$
\end_inset

 again.
 This gives 
\begin_inset Formula $T$
\end_inset

 the structure of a non-commutative (but unital) DG 
\begin_inset Formula $\Bbbk$
\end_inset

-algebra, where
\begin_inset Formula 
\[
\Bbbk=\{r\in R\mid\mathrm{d}r\otimes a=0\text{ for all }a\in A\}.
\]

\end_inset

In other words, the differential of 
\begin_inset Formula $T$
\end_inset

 satisfies Leibniz law and is 
\begin_inset Formula $\Bbbk$
\end_inset

-linear.
 Note that the generator 
\begin_inset Formula $[r,a]_{\mu}$
\end_inset

 of 
\begin_inset Formula $\mathfrak{u}$
\end_inset

 is also homogeneous with respect to total degree, however the generators
 
\begin_inset Formula $[r,a]_{\mathrm{d}}$
\end_inset

 is homogeneous with respect to total degree if and only if either 
\begin_inset Formula $\mathrm{d}r\otimes a=0$
\end_inset

, or 
\begin_inset Formula $\mathrm{d}(ra)=r\mathrm{d}a$
\end_inset

, or 
\begin_inset Formula $|a|\in\{0,1\}$
\end_inset

.
 In particular, 
\begin_inset Formula $\mathfrak{u}$
\end_inset

 will be homogeneous with respect to total degree if 
\begin_inset Formula $A$
\end_inset

 is an 
\begin_inset Formula $R$
\end_inset

-complex centered at 
\begin_inset Formula $R$
\end_inset

 (which is a case we are interested in).
 In this case, 
\begin_inset Formula $T$
\end_inset

 inherits from 
\begin_inset Formula $U$
\end_inset

 a bi-graded 
\begin_inset Formula $R$
\end_inset

-algebra structure:
\begin_inset Formula 
\[
T=\bigoplus_{i\in\mathbb{Z}}T_{i}=\bigoplus_{m\in\mathbb{Z}}T^{m}=\bigoplus_{i,m\in\mathbb{Z}}T_{i}^{m}.
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{example}
\backslash
label{example}
\end_layout

\end_inset

 Let us describe what the total degree 
\begin_inset Formula $m$
\end_inset

 component of 
\begin_inset Formula $T=\mathrm{T}_{R}(A)$
\end_inset

 in the case where 
\begin_inset Formula $A$
\end_inset

 is an 
\begin_inset Formula $R$
\end_inset

-complex centered at 
\begin_inset Formula $R$
\end_inset

.
 We have
\begin_inset Formula 
\begin{align*}
T^{0} & =R\\
T^{1} & =\bigoplus_{1\leq i}A_{i}\\
T^{2} & =\bigoplus_{\substack{1\leq i<j}
}((A_{i}\otimes A_{j})\oplus(A_{j}\otimes A_{i}))\oplus\bigoplus_{1\leq i}A_{i}^{\otimes2}
\end{align*}

\end_inset

The component 
\begin_inset Formula $T^{3}$
\end_inset

 is slightly more complicated:
\begin_inset Formula 
\[
\bigoplus_{\substack{1\leq i<j<k\\
\pi\in S_{3}
}
}(A_{\pi(i)}\otimes A_{\pi(j)}\otimes A_{\pi(k)})\oplus\bigoplus_{\substack{1\leq i<j\\
\pi\in S_{2}
}
}((A_{\pi(i)}^{\otimes2}\otimes A_{\pi(j)})\oplus(A_{\pi(i)}\otimes A_{\pi(j)}\otimes A_{\pi(i)})\oplus(A_{\pi(i)}\otimes A_{\pi(j)}^{\otimes2}))\oplus\bigoplus_{1\leq i}A_{i}^{\otimes3}.
\]

\end_inset

More generally, there is an interpretation of 
\begin_inset Formula $T^{m}$
\end_inset

 in terms of certain rooted trees.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{example}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

Now let 
\begin_inset Formula $\mathfrak{t}=\mathfrak{t}(A)$
\end_inset

 be the 
\begin_inset Formula $T$
\end_inset

-ideal generated by all elements of the form
\begin_inset Formula 
\[
[a_{1},a_{2}]_{\sigma}\colon=(-1)^{|a_{1}||a_{2}|}a_{2}\otimes a_{1}-a_{1}\otimes a_{2}\quad\text{and}\quad[a]_{\tau}:=a\otimes a,
\]

\end_inset

where 
\begin_inset Formula $a,a_{1},a_{2}\in A$
\end_inset

 are homogeneous and 
\begin_inset Formula $|a|$
\end_inset

 is odd.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{lemma}
\backslash
label{lemma}
\end_layout

\end_inset

 The differential of 
\begin_inset Formula $T$
\end_inset

 maps 
\begin_inset Formula $\mathfrak{t}$
\end_inset

 to itself.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{lemma}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{proof}
\backslash
label{proof}
\end_layout

\end_inset

 Indeed, if 
\begin_inset Formula $a,a_{1},a_{2}\in A$
\end_inset

 are homogeneous with 
\begin_inset Formula $|a|$
\end_inset

 odd, then we have
\begin_inset Formula 
\begin{align*}
\mathrm{d}[a_{1},a_{2}]_{\sigma} & =[\mathrm{d}a_{1},a_{2}]_{\sigma}+(-1)^{|a_{1}|}[a_{1},\mathrm{d}a_{2}]_{\sigma}\in\mathfrak{t}\quad\text{and}\quad\mathrm{d}[a]_{\tau}=[\mathrm{d}a,a]_{\sigma}\in\mathfrak{t}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{proof}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
hfill
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\series bold
Step 3: 
\series default
We define the 
\series bold
symmetric DG algebra 
\series default
of 
\begin_inset Formula $A$
\end_inset

 to be the quotient
\begin_inset Formula 
\[
\mathrm{S}(A):=\mathrm{T}(A)\slash\mathfrak{t}(A)
\]

\end_inset

The image of a homogeneous elementary tensor 
\begin_inset Formula $a_{1}\otimes\cdots\otimes a_{m}$
\end_inset

 in 
\begin_inset Formula $S=\mathrm{S}(A)$
\end_inset

 is often denoted 
\begin_inset Formula $a_{1}\cdots a_{n}$
\end_inset

 and is called a homogeneous elementary product.
 Since 
\begin_inset Formula $\mathfrak{t}=\mathfrak{t}(A)$
\end_inset

 is generated by elements which are homogeneous with respect to both homological
 degree and since the differential of 
\begin_inset Formula $T=\mathrm{T}(A)$
\end_inset

 maps 
\begin_inset Formula $\mathfrak{t}$
\end_inset

 to itself, we see that the differential of 
\begin_inset Formula $T$
\end_inset

 induces a differential on 
\begin_inset Formula $S$
\end_inset

, which we again denote by 
\begin_inset Formula $\mathrm{d}$
\end_inset

, giving it the structure of a strictly graded-commutative DG 
\begin_inset Formula $\Bbbk$
\end_inset

-algebra.
 Furthemore, if 
\begin_inset Formula $T$
\end_inset

 inherits the bi-graded structure from 
\begin_inset Formula $U$
\end_inset

, then 
\begin_inset Formula $S$
\end_inset

 inherits the bi-graded structure from 
\begin_inset Formula $T$
\end_inset

 since 
\begin_inset Formula $\mathfrak{t}$
\end_inset

 is generated by elements which are homogeneous with respect to total degree.
\end_layout

\begin_layout Standard
\begin_inset Branch Temporary
inverted 0
status collapsed

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{example}
\backslash
label{example}
\end_layout

\end_inset

 Let 
\begin_inset Formula $R=\Bbbk[x_{1},\dots,x_{n}]=\Bbbk[\boldsymbol{x}]$
\end_inset

 and let 
\begin_inset Formula $\overline{R}=R\slash\langle f_{1},\dots,f_{m}\rangle=R\slash\langle\boldsymbol{f}\rangle$
\end_inset

.
 Let 
\begin_inset Formula $U$
\end_inset

 be the graded 
\begin_inset Formula $R$
\end_inset

-module whose component in degree 
\begin_inset Formula $k$
\end_inset

 is given by
\begin_inset Formula 
\[
U_{k}=\begin{cases}
\bigoplus_{i=1}^{m}e_{i}R & k=1\\
R & k=0\\
\bigoplus_{i=1}^{n}R\varepsilon_{i}\slash\langle\partial f_{1},\dots,\partial f_{m}\rangle & k=-1\\
\Lambda_{R}^{-k}(U_{-1}) & \text{if }-2\ge k\geq-n\\
0 & \text{else}
\end{cases}
\]

\end_inset

where 
\begin_inset Formula $\partial f=\sum_{j=1}^{n}(\partial_{x_{j}}f)\varepsilon_{j}$
\end_inset

 is the usual gradient of 
\begin_inset Formula $f$
\end_inset

 expressed in the 
\begin_inset Formula $\boldsymbol{\varepsilon}$
\end_inset

-basis.
 We define 
\begin_inset Formula $\mathrm{d}\colon U\to U$
\end_inset

 to be the unique 
\begin_inset Formula $\Bbbk$
\end_inset

-linear differential given by
\begin_inset Formula 
\[
\mathrm{d}(e_{i}^{\varepsilon}x^{\boldsymbol{\alpha}})=\begin{cases}
f_{i} & \text{if }\varepsilon=1\text{ and }\boldsymbol{\alpha}=0\\
0 & \text{if }\varepsilon=1\text{ and }\boldsymbol{\alpha}\neq0\\
\overline{\partial(x^{\boldsymbol{\alpha}})} & \text{if }\varepsilon=0.
\end{cases}
\]

\end_inset

where 
\begin_inset Formula $1\leq i\leq m$
\end_inset

 , where 
\begin_inset Formula $\boldsymbol{\alpha}\in\mathbb{N}^{n}$
\end_inset

, and where 
\begin_inset Formula $\varepsilon\in\{0,1\},$
\end_inset

 and such that
\begin_inset Formula 
\[
\mathrm{d}(x^{\boldsymbol{\alpha}}\varepsilon_{\sigma})=\partial(x^{\boldsymbol{\alpha}})\varepsilon_{\sigma}
\]

\end_inset

for all 
\begin_inset Formula $\sigma\subseteq\{1,2,.\dots,n\}$
\end_inset

.
 Note that 
\begin_inset Formula $(U\otimes_{R}\overline{R})_{\leq0}=\Omega_{\overline{R}\slash\Bbbk}$
\end_inset

 is just the de Rham complex of 
\begin_inset Formula $\overline{R}$
\end_inset

 over 
\begin_inset Formula $\Bbbk$
\end_inset

.
 In particular, we have
\begin_inset Formula 
\[
\mathrm{H}(U\otimes_{R}\overline{R})=\bigoplus_{i=1}^{m}e_{i}\overline{R}\oplus\mathrm{H}_{\mathrm{dR}}(\Omega_{\overline{R}\slash\Bbbk}).
\]

\end_inset

Now let 
\begin_inset Formula $\mathrm{S}=\mathrm{S}(U)$
\end_inset

 be the symmetric DG algebra of 
\begin_inset Formula $\mathrm{U}$
\end_inset

.
 Then in 
\begin_inset Formula $\mathrm{S}$
\end_inset

, we have the relations 
\begin_inset Formula $e_{i}\varepsilon_{j}=f_{i}x_{j}$
\end_inset

 for all 
\begin_inset Formula $i,j$
\end_inset

, thus in 
\begin_inset Formula $\mathrm{S}\otimes_{R}S=\mathrm{S}(U\otimes_{R}S)$
\end_inset

 we get the relations 
\begin_inset Formula $e_{i}\varepsilon_{j}=0$
\end_inset

 for all 
\begin_inset Formula $i,j$
\end_inset

.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{example}
\end_layout

\end_inset


\end_layout

\end_inset

 
\begin_inset Branch Temporary
inverted 0
status collapsed

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{example}
\backslash
label{example}
\end_layout

\end_inset

 Consider the case where 
\begin_inset Formula $\Bbbk$
\end_inset

 is a field with 
\begin_inset Formula $\mathrm{char}\,\Bbbk\neq2$
\end_inset

, where 
\begin_inset Formula $R=\Bbbk[x,y]$
\end_inset

 and where 
\begin_inset Formula $S=R\slash\langle x^{2},xy\rangle$
\end_inset

.
 Let 
\begin_inset Formula $F$
\end_inset

 be the usual Taylor algebra resolution of 
\begin_inset Formula $S$
\end_inset

 over 
\begin_inset Formula $R$
\end_inset

.
 Let 
\begin_inset Formula $U=G\slash\mathfrak{b}$
\end_inset

 as above.
 Then note that
\begin_inset Formula 
\begin{align*}
U_{-2} & =R\varepsilon_{1}\varepsilon_{2}\\
U_{-1} & =R\varepsilon_{1}+R\varepsilon_{2}+e_{1}R\varepsilon_{1}\varepsilon_{2}+e_{2}R\varepsilon_{1}\varepsilon_{2}\\
U_{0} & =R+e_{1}R\varepsilon_{1}+e_{1}R\varepsilon_{2}+e_{1}e_{2}R\varepsilon_{1}\varepsilon_{2}+e_{2}R\varepsilon_{1}+e_{2}R\varepsilon_{2}\\
U_{1} & =e_{1}R+e_{2}R+e_{1}e_{2}R\varepsilon_{1}+e_{1}e_{2}R\varepsilon_{2}\\
U_{2} & =e_{1}e_{2}R.
\end{align*}

\end_inset

Now let 
\begin_inset Formula $V=G\slash\langle\mathfrak{a},\mathfrak{b}\rangle$
\end_inset

 as above.
 Note that
\begin_inset Formula 
\begin{align*}
a_{1} & =2x\varepsilon_{1} & c_{1,j} & =e_{1}\widetilde{\varepsilon}_{j}-x^{2}\\
a_{2} & =y\varepsilon_{2}+x\varepsilon_{1} & c_{2,j} & =e_{2}\widetilde{\varepsilon}_{j}-xy\\
a_{12} & =-e_{2}\varepsilon_{1}+e_{1}\varepsilon_{2} & c_{12,j} & =e_{12}\widetilde{\varepsilon}_{j}-xe_{2}+ye_{1}
\end{align*}

\end_inset

In particular, in 
\begin_inset Formula $V$
\end_inset

 we have 
\begin_inset Formula $x\varepsilon_{2}=-y\varepsilon_{1}$
\end_inset

 and
\begin_inset Formula 
\begin{align*}
0:_{R}\varepsilon_{2} & =\langle x^{2},xy\rangle\\
0:_{R}\varepsilon_{1} & =\langle x\rangle\\
0:_{R}\varepsilon_{1}\varepsilon_{2} & =\langle x,y\rangle.
\end{align*}

\end_inset

So we can express the components of 
\begin_inset Formula $V$
\end_inset

 in a better way using these relations.
 Indeed, set 
\begin_inset Formula $T=\Bbbk[y]$
\end_inset

 and note that 
\begin_inset Formula $S=T+xT$
\end_inset

, thus one can see that 
\begin_inset Formula $R\varepsilon_{2}=S\varepsilon_{2}$
\end_inset

, 
\begin_inset Formula $R\varepsilon_{1}=T\varepsilon_{1}$
\end_inset

, and 
\begin_inset Formula $R\varepsilon_{1}\varepsilon_{2}=\Bbbk$
\end_inset

.
 Using annihilations like these, one can show that
\begin_inset Formula 
\begin{align*}
V_{-2} & =\Bbbk\varepsilon_{1}\varepsilon_{2}\\
V_{-1} & =T\varepsilon_{1}+T\varepsilon_{2}+e_{1}\Bbbk\varepsilon_{1}\varepsilon_{2}+e_{2}\Bbbk\varepsilon_{1}\varepsilon_{2}\\
V_{0} & =R+e_{1}T\varepsilon_{1}+e_{1}T\varepsilon_{2}+e_{2}T\varepsilon_{1}+e_{2}T\varepsilon_{2}+e_{1}e_{2}\Bbbk\varepsilon_{1}\varepsilon_{2}\\
V_{1} & =e_{1}R+e_{2}R+e_{1}e_{2}T\varepsilon_{1}+e_{1}e_{2}T\varepsilon_{2}\\
V_{2} & =e_{1}e_{2}R.
\end{align*}

\end_inset

Finally let 
\begin_inset Formula $W=G\slash\langle\mathfrak{a},\mathfrak{b},\mathfrak{c}\rangle$
\end_inset

.
 We set 
\begin_inset Formula $\widetilde{\varepsilon}_{j}=\varepsilon_{j}/(x_{j}-1)$
\end_inset

.
 In 
\begin_inset Formula $W$
\end_inset

, we have
\begin_inset Formula 
\begin{align*}
e_{1}\widetilde{\varepsilon}_{j} & =x^{2} & e_{1}x^{2} & =0 & x^{3} & =0\\
e_{2}\widetilde{\varepsilon}_{j} & =xy & e_{2}xy & =0 & x^{2}y & =0\\
e_{1}y=e_{12}\widetilde{\varepsilon}_{j} & =-e_{2}x & e_{2}x^{2} & =-e_{1}xy
\end{align*}

\end_inset

We have
\begin_inset Formula 
\begin{align*}
e_{1}\varepsilon_{1} & =x^{3} & x^{3}e_{1} & =0 & e_{1}\varepsilon_{1}\varepsilon_{2} & =0 & x^{4} & =0\\
e_{1}\varepsilon_{2} & =x^{2}y & x^{2}ye_{1} & =0 & e_{2}\varepsilon_{1}\varepsilon_{2} & =0 & x^{3}y & =0\\
e_{2}\varepsilon_{1} & =x^{2}y & x^{2}ye_{2} & =0 & e_{1}e_{2}\varepsilon_{1} & =0 & x^{2}y^{3} & =0\\
e_{2}\varepsilon_{2} & =xy^{2} & xy^{2}e_{2} & =0 & e_{1}e_{2}\varepsilon_{2} & =0 & e_{1}e_{2}\varepsilon_{1}\varepsilon_{2} & =0
\end{align*}

\end_inset

Furthermore we have
\begin_inset Formula 
\begin{align*}
0:_{R}e_{1} & =\langle x^{3},x^{2}y,xy^{2}\rangle=\langle x\rangle\langle x,y\rangle^{2}\\
0:_{R}e_{2} & =\langle x^{3},x^{2}y,xy^{2}\rangle=\langle x\rangle\langle x,y\rangle^{2}\\
0:_{R}e_{1}e_{2} & =\langle x^{3},x^{2}y,xy^{2}\rangle=\langle x\rangle\langle x,y\rangle^{2}.
\end{align*}

\end_inset

Therefore if
\begin_inset Formula 
\[
Q=R\slash\langle x^{3},x^{2}y,xy^{2}\rangle=\Bbbk+\Bbbk x+\Bbbk y+\Bbbk x^{2}+\Bbbk xy+\Bbbk y^{2}+\Bbbk y^{3}\cong\Bbbk\oplus(\Bbbk+\Bbbk x+\Bbbk y),
\]

\end_inset

then we have
\begin_inset Formula 
\begin{align*}
\overline{V}_{-2} & =\Bbbk\varepsilon_{1}\varepsilon_{2}\\
\overline{V}_{-1} & =T\varepsilon_{1}+T\varepsilon_{2}\\
\overline{V}_{0} & =T+Tx+\Bbbk x^{2}+\Bbbk x^{2}y+\Bbbk x^{2}y^{2}+\Bbbk x^{3}\\
\overline{V}_{1} & =e_{1}T+e_{2}T+e_{1}Q+e_{2}Q+e_{2}x^{3}\Bbbk\\
\overline{V}_{2} & =e_{1}e_{2}T+e_{1}e_{2}Q.
\end{align*}

\end_inset

Note that 
\begin_inset Formula $\mathrm{d}(x^{2})=2x\varepsilon_{1}=0$
\end_inset

 and 
\begin_inset Formula $\mathrm{d}(x^{2}y)=x^{2}\varepsilon_{2}=0$
\end_inset

.
 Also note
\begin_inset Formula 
\begin{align*}
\mathrm{d}(e_{1}e_{2}) & =x^{2}e_{2}-xye_{1}\\
\mathrm{d}(e_{1}e_{2}x) & =e_{2}x^{3}\\
\mathrm{d}(e_{1}e_{2}y) & =-e_{1}xy^{2}\\
\mathrm{d}(e_{1}e_{2}xy) & =0\\
\mathrm{d}(e_{1}e_{2}y^{2}) & =0\\
\mathrm{d}(e_{1}e_{2}x^{2}) & =0
\end{align*}

\end_inset

Therefore we see that
\begin_inset Formula 
\[
\mathrm{H}_{2}(\overline{V})=e_{1}e_{2}\Bbbk x^{2}+e_{1}e_{2}\Bbbk xy+e_{1}e_{2}Ty^{2}\cong\Bbbk^{2}\oplus\langle y^{2}\rangle\cong
\]

\end_inset


\end_layout

\begin_layout Standard
We also have
\begin_inset Formula 
\begin{align*}
\mathrm{d}(e_{1}xy) & =\mathrm{d}(e_{1})
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{example}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Finally, Observe that
\begin_inset Formula 
\[
V\slash\langle\boldsymbol{\varepsilon}\rangle\simeq\mathrm{S}_{R}(F)=\mathrm{S}(F)\quad\text{and}\quad V\slash\langle\boldsymbol{e},\boldsymbol{f}\rangle\simeq\Omega_{S\slash\Bbbk},
\]

\end_inset

where 
\begin_inset Formula $\mathrm{S}_{R}(F)$
\end_inset

 is the usual symmetric DG algebra construction applied to 
\begin_inset Formula $F$
\end_inset

 and where 
\begin_inset Formula $\Omega_{S\slash\Bbbk}$
\end_inset

 is algebraic de Rham of 
\begin_inset Formula $S\slash\Bbbk$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

Now for each 
\begin_inset Formula $1\leq j\leq n$
\end_inset

, choose a unit 
\begin_inset Formula $u_{j}\in R^{\times}$
\end_inset

 such that 
\begin_inset Formula $\mathrm{d}u_{j}=\varepsilon_{j}$
\end_inset

 (for instance if 
\begin_inset Formula $R$
\end_inset

 is local, then we may choose 
\begin_inset Formula $u_{j}=x_{j}+1$
\end_inset

) and we set 
\begin_inset Formula $\widetilde{\varepsilon}_{j}=\varepsilon_{j}/u_{j}$
\end_inset

.
 Thus
\begin_inset Formula 
\begin{align*}
\mathrm{d}(\widetilde{\varepsilon}_{j}) & =\frac{-\varepsilon_{j}^{2}}{u_{j}^{2}}=0
\end{align*}

\end_inset

Let 
\begin_inset Formula $\mathfrak{b}$
\end_inset

 be the DG 
\begin_inset Formula $V$
\end_inset

-ideal generated by all elements of the form 
\begin_inset Formula $e_{i}\varepsilon_{j}-(\mathrm{d}e_{i})u_{j}$
\end_inset

 for all 
\begin_inset Formula $i,j$
\end_inset

.
 Note that
\begin_inset Formula 
\begin{align*}
\mathrm{d}(e_{i}\varepsilon_{j}-(\mathrm{d}e_{i})u_{j}) & =(\mathrm{d}e_{i})\varepsilon_{j}-(\mathrm{d}e_{i})(\mathrm{d}u_{j})\\
 & =(\mathrm{d}e_{i})\varepsilon_{j}-(\mathrm{d}e_{i})\varepsilon_{j}\\
 & =0.
\end{align*}

\end_inset

 Note that
\begin_inset Formula 
\begin{align*}
\mathrm{d}(e_{i}\widetilde{\varepsilon}_{j}-\mathrm{d}e_{i}) & =(\mathrm{d}e_{i})\widetilde{\varepsilon}_{j}-e_{i}\mathrm{d}(\widetilde{\varepsilon}_{j})\\
 & =\\
 & =0.
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\mathrm{d}(e_{i}\widetilde{\varepsilon}_{j}) & =\mathrm{d}(e_{i})\widetilde{\varepsilon}_{j}\\
\\
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\mathrm{d}(e_{i}\varepsilon_{j}-\mathrm{d}e_{i}) & =\mathrm{d}(e_{i})\varepsilon_{j}
\end{align*}

\end_inset


\begin_inset Formula 
\[
e_{i}\varepsilon_{j}-\mathrm{d}e_{i}
\]

\end_inset


\end_layout

\begin_layout Standard
Now let 
\begin_inset Formula $\mathfrak{b}$
\end_inset

 be the DG 
\begin_inset Formula $V$
\end_inset

-ideal generated by all elements of the form 
\begin_inset Formula $e_{i}$
\end_inset


\end_layout

\begin_layout Standard
As 
\begin_inset Formula $F$
\end_inset

 was assumed to have a DG algebra structure on it with multiplication denoted
 
\begin_inset Formula $\star$
\end_inset

, we can recover 
\begin_inset Formula $F$
\end_inset

 again by quotienting out by 
\begin_inset Formula $\mathrm{S}(F)$
\end_inset

 ideal of multiplicators
\begin_inset Formula 
\[
\mathfrak{b}=\langle\{e_{i}e_{j}-e_{i}\star e_{j}\mid1\leq i,j\leq N\}\rangle.
\]

\end_inset


\end_layout

\begin_layout Standard
We equip 
\begin_inset Formula $V$
\end_inset

 with a multiplication 
\begin_inset Formula $\nu$
\end_inset

 which is defined by
\begin_inset Formula 
\[
e_{i}\varepsilon_{j}=f_{i}(x_{j}-1)=f_{i}u_{j},
\]

\end_inset

where we denote 
\begin_inset Formula $x_{j}-1=u_{j}$
\end_inset

 for all 
\begin_inset Formula $i,j$
\end_inset

.
 This multiplication is not associative for we have
\begin_inset Formula 
\begin{align*}
0 & =[e_{i},\widetilde{\varepsilon}_{k},e_{j}]=e_{j}f_{i}+e_{i}f_{j}\\
0 & =[e_{i},\widetilde{\varepsilon}_{j},\widetilde{\varepsilon}_{j}]=f_{i}\widetilde{\varepsilon}_{j}\\
0 & =[\widetilde{\varepsilon}_{j},e_{i},e_{i}]=e_{i}f_{i}.
\end{align*}

\end_inset

In particular, note that 
\begin_inset Formula $\langle\boldsymbol{f}\rangle$
\end_inset

 annihilates 
\begin_inset Formula $\langle\boldsymbol{\varepsilon}\rangle$
\end_inset

.
 We have identities
\begin_inset Formula 
\[
e_{j}f_{i}=-f_{i}e_{j},\quad e_{i}f_{i}=0,\quad\text{and}\quad f_{i}\varepsilon_{j}=0
\]

\end_inset

for all 
\begin_inset Formula $i,j$
\end_inset

.
 
\begin_inset Formula 
\begin{align*}
e_{j}f_{i} & =-f_{i}e_{j}\\
e_{i}f_{i} & =0\\
f_{i}\varepsilon_{j} & =0
\end{align*}

\end_inset

for all 
\begin_inset Formula $ $
\end_inset


\begin_inset Formula 
\begin{align*}
[e_{i},\varepsilon_{j},e_{k}] & =u_{j}(f_{i}e_{k}+f_{k}e_{i})\\
0=[\varepsilon_{i},e_{j},\varepsilon_{k}] & =-f_{j}(u_{i}e_{k}+u_{k}e_{i})\\
0=[\widetilde{\varepsilon}_{j},e_{i},e_{i}] & =f_{i}e_{i}\\
0=[e_{i},\widetilde{\varepsilon}_{j},\widetilde{\varepsilon}_{j}] & =f_{i}\widetilde{\varepsilon}_{j}
\end{align*}

\end_inset

for all 
\begin_inset Formula $i,j,k$
\end_inset

.
 Thus in the maximal associative quotient 
\begin_inset Formula $\overline{V}=V\slash\langle V\rangle$
\end_inset

 we get the relations of the form
\begin_inset Formula 
\begin{align*}
\overline{u}_{j}\overline{f}_{i}\overline{e}_{k} & =-\overline{u}_{j}\overline{f}_{k}\overline{e}_{i}\\
\overline{f}_{j}\overline{u}_{i}\overline{e}_{k} & =-\overline{f}_{j}\overline{u}_{k}\overline{e}_{i}\\
\overline{u}_{i}\overline{f}_{j}\overline{\varepsilon}_{i} & =0\\
\overline{u}_{i}\overline{f}_{j}\overline{\varepsilon}_{j} & =0.
\end{align*}

\end_inset

It turns out that under suitable conditions we have 
\begin_inset Formula $\mathrm{H}_{+}(\overline{V})=\mathrm{H}_{+}(F\otimes_{R}S)$
\end_inset

 and 
\begin_inset Formula $\mathrm{H}_{-}(\overline{V})=\mathrm{H}_{\mathrm{d}\mathrm{R}}(\Omega_{S\slash\Bbbk})$
\end_inset

!
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{theorem}
\backslash
label{theorem}
\end_layout

\end_inset

 Suppose each 
\begin_inset Formula $u_{j}$
\end_inset

 is a unit (we can achieve this by localizing at the prime 
\begin_inset Formula $\langle\boldsymbol{x}\rangle$
\end_inset

 or by replacing 
\begin_inset Formula $u_{j}$
\end_inset

 with 
\begin_inset Formula $v_{j}=a_{j}x_{j}+b_{j}$
\end_inset

 for some 
\begin_inset Formula $a_{j}\in\Bbbk^{\times}$
\end_inset

 and 
\begin_inset Formula $b_{j}\in\Bbbk$
\end_inset

 and localizing at the multiplicative set generated by the 
\begin_inset Formula $v_{j}$
\end_inset

).
 Then we have
\begin_inset Formula 
\[
\mathrm{H}_{+}(\overline{V})=\mathrm{H}_{+}(F\otimes_{R}S).
\]

\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{theorem}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{proof}
\backslash
label{proof}
\end_layout

\end_inset

 Recall that 
\begin_inset Formula $V\slash\langle\boldsymbol{f},\boldsymbol{\varepsilon}\rangle\simeq F\otimes_{R}S$
\end_inset

.
 Let 
\begin_inset Formula $\mathfrak{c}$
\end_inset

 be the DG 
\begin_inset Formula $V$
\end_inset

-ideal generated by the 
\begin_inset Formula $e_{i}\varepsilon_{j}-f_{i}u_{j}$
\end_inset

.
 Note that since 
\begin_inset Formula $u_{j}$
\end_inset

 is a unit, we have
\begin_inset Formula 
\[
e_{i}\varepsilon_{j}-f_{i}u_{j}\in\langle\boldsymbol{f},\boldsymbol{\varepsilon}\rangle.
\]

\end_inset

It follows that 
\begin_inset Formula $\langle\mathfrak{c},\boldsymbol{\varepsilon}\rangle=\langle\boldsymbol{f},\boldsymbol{\varepsilon}\rangle$
\end_inset

.
 Thus we can quotient out by 
\begin_inset Formula $\mathfrak{c}$
\end_inset

 first to get 
\begin_inset Formula $\overline{V}=V\slash\mathfrak{c}$
\end_inset

, and then we just need to quotient out by 
\begin_inset Formula $\langle\boldsymbol{\varepsilon}\rangle$
\end_inset

 to get 
\begin_inset Formula $F$
\end_inset

.
 However we claim that quotienting out by 
\begin_inset Formula $\langle\boldsymbol{\varepsilon}\rangle$
\end_inset

 will not affect 
\begin_inset Formula $\overline{V}_{+}$
\end_inset

.
 Indeed, by using the multiplication, we can place 
\begin_inset Formula $\overline{V}_{+}$
\end_inset

 into the form
\begin_inset Formula 
\[
\overline{V}_{+}=\bigoplus_{\boldsymbol{\alpha},\boldsymbol{\beta}}e^{\boldsymbol{\alpha}}\Bbbk x^{\boldsymbol{\beta}},
\]

\end_inset

where 
\begin_inset Formula $\boldsymbol{\alpha},\boldsymbol{\beta}$
\end_inset

 do not necessarily range over all 
\begin_inset Formula $\mathbb{N}^{n}$
\end_inset

.
 We have
\begin_inset Formula 
\begin{align*}
e_{i}\mathrm{d}(x^{\boldsymbol{\alpha}}) & =\alpha_{1}x_{1}^{\alpha_{1}-1}x_{2}^{\alpha_{2}}\cdots x_{n}^{\alpha_{n}}e_{i}\varepsilon_{1}+\sum_{j=2}^{n}\alpha_{j}x_{1}^{\alpha_{1}}\cdots x_{j-1}^{\alpha_{j-1}}x_{j}^{\alpha_{j}-1}\cdots x_{n}^{\alpha_{n}}e_{i}\varepsilon_{j}\\
 & =\alpha_{1}f_{i}x_{1}^{\alpha_{1}}x_{2}^{\alpha_{2}}\cdots x_{n}^{\alpha_{n}}+\sum_{j=2}^{n}\alpha_{j}f_{i}x_{1}^{\alpha_{1}}\cdots x_{j-1}^{\alpha_{j-1}}x_{j}^{\alpha_{j}}\cdots x_{n}^{\alpha_{n}}\\
 & =|\boldsymbol{\alpha}|f_{i}x^{\boldsymbol{\alpha}}.
\end{align*}

\end_inset

Therefore if 
\begin_inset Formula $e^{\boldsymbol{\alpha}}=e^{\widetilde{\boldsymbol{\alpha}}}e_{l}$
\end_inset

, then we have
\begin_inset Formula 
\begin{align*}
\mathrm{d}(e^{\boldsymbol{\alpha}}x^{\boldsymbol{\beta}}) & =\mathrm{d}(e^{\boldsymbol{\alpha}})x^{\boldsymbol{\beta}}\pm e^{\boldsymbol{\alpha}}\mathrm{d}(x^{\boldsymbol{\beta}})\\
 & =\mathrm{d}(e^{\boldsymbol{\alpha}})x^{\boldsymbol{\beta}}\pm|\boldsymbol{\beta}|e^{\widetilde{\boldsymbol{\alpha}}}f_{l}x^{\boldsymbol{\beta}}\\
 & =(\mathrm{d}(e^{\boldsymbol{\alpha}})\pm|\boldsymbol{\beta}|e^{\widetilde{\boldsymbol{\alpha}}}f_{l})x^{\boldsymbol{\beta}}\\
 & =(\mathrm{d}(e^{\widetilde{\boldsymbol{\alpha}}}e_{l})\pm|\boldsymbol{\beta}|e^{\widetilde{\boldsymbol{\alpha}}}f_{l})x^{\boldsymbol{\beta}}\\
 & =(\mathrm{d}(e^{\widetilde{\boldsymbol{\alpha}}})e_{l}\pm e^{\widetilde{\boldsymbol{\alpha}}}f_{l}\pm|\boldsymbol{\beta}|e^{\widetilde{\boldsymbol{\alpha}}}f_{l})x^{\boldsymbol{\beta}}\\
 & =(\mathrm{d}(e^{\widetilde{\boldsymbol{\alpha}}})e_{l}\pm(1\pm|\boldsymbol{\beta}|)e^{\widetilde{\boldsymbol{\alpha}}}f_{l})x^{\boldsymbol{\beta}}.\\
 & =(\mathrm{d}(e^{\widetilde{\boldsymbol{\alpha}}})e_{l}x^{\boldsymbol{\beta}}\pm(1\pm|\boldsymbol{\beta}|)e^{\widetilde{\boldsymbol{\alpha}}}f_{l}x^{\boldsymbol{\beta}}
\end{align*}

\end_inset

thus suppose 
\begin_inset Formula $v$
\end_inset

 is a cycle in 
\begin_inset Formula $\overline{V}$
\end_inset

, say 
\begin_inset Formula $v=\sum c_{i}e^{\boldsymbol{\alpha}_{i}}x^{\boldsymbol{\beta}_{i}}$
\end_inset

.
 Then we have
\begin_inset Formula 
\begin{align*}
0 & =\mathrm{d}v\\
 & =\sum c_{i}\mathrm{d}(e^{\boldsymbol{\alpha}_{i}}x^{\boldsymbol{\beta}_{i}})\\
 & =\sum c_{i}\mathrm{d}(e^{\boldsymbol{\alpha}})x^{\boldsymbol{\beta}}\pm\sum c_{i}e^{\boldsymbol{\alpha}}\mathrm{d}(x^{\boldsymbol{\beta}})).
\end{align*}

\end_inset

It follows that 
\begin_inset Formula $\sum c_{i}\mathrm{d}(e^{\boldsymbol{\alpha}})x^{\boldsymbol{\beta}}=0$
\end_inset

.
 Furthermore, note that 
\begin_inset Formula $\langle\boldsymbol{e}\rangle\langle\boldsymbol{\varepsilon}\rangle+\langle\boldsymbol{c}\rangle=\langle\boldsymbol{f}\rangle$
\end_inset

.
 We have
\begin_inset Formula 
\begin{align*}
e_{i}\mathrm{d}(x^{\boldsymbol{\alpha}}) & =\alpha_{1}x_{1}^{\alpha_{1}-1}x_{2}^{\alpha_{2}}\cdots x_{n}^{\alpha_{n}}e_{i}\varepsilon_{1}+\sum_{j=2}^{n}\alpha_{j}x_{1}^{\alpha_{1}}\cdots x_{j-1}^{\alpha_{j-1}}x_{j}^{\alpha_{j}-1}\cdots x_{n}^{\alpha_{n}}e_{i}\varepsilon_{j}\\
 & =\alpha_{1}f_{i}x_{1}^{\alpha_{1}}x_{2}^{\alpha_{2}}\cdots x_{n}^{\alpha_{n}}+\sum_{j=2}^{n}\alpha_{j}f_{i}x_{1}^{\alpha_{1}}\cdots x_{j-1}^{\alpha_{j-1}}x_{j}^{\alpha_{j}}\cdots x_{n}^{\alpha_{n}}\\
 & =|\boldsymbol{\alpha}|f_{i}x^{\boldsymbol{\alpha}}.
\end{align*}

\end_inset

We have
\begin_inset Formula 
\begin{align*}
e_{i}\mathrm{d}(x^{\boldsymbol{\alpha}}) & =\alpha_{1}x_{1}^{\alpha_{1}-1}x_{2}^{\alpha_{2}}\cdots x_{n}^{\alpha_{n}}e_{i}\varepsilon_{1}+\sum_{j=2}^{n}\alpha_{j}x_{1}^{\alpha_{1}}\cdots x_{j-1}^{\alpha_{j-1}}x_{j}^{\alpha_{j}-1}\cdots x_{n}^{\alpha_{n}}e_{i}\varepsilon_{j}\\
 & =\alpha_{1}f_{i}x_{1}^{\alpha_{1}}x_{2}^{\alpha_{2}}\cdots x_{n}^{\alpha_{n}}+\sum_{j=2}^{n}\alpha_{j}f_{i}x_{1}^{\alpha_{1}}\cdots x_{j-1}^{\alpha_{j-1}}x_{j}^{\alpha_{j}}\cdots x_{n}^{\alpha_{n}}\\
 & =|\boldsymbol{\alpha}|f_{i}x^{\boldsymbol{\alpha}}.
\end{align*}

\end_inset

We have
\begin_inset Formula 
\begin{align*}
e_{j}e_{i}\mathrm{d}(x^{\boldsymbol{\alpha}}) & =|\boldsymbol{\alpha}|f_{i}e_{j}x^{\boldsymbol{\alpha}}.
\end{align*}

\end_inset

More generally we have
\begin_inset Formula 
\begin{align*}
e^{\boldsymbol{\alpha}}\mathrm{d}(x^{\boldsymbol{\beta}}) & =
\end{align*}

\end_inset

 
