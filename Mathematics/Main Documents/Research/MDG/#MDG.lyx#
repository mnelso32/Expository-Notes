\backslash
  & 
\backslash
mathrm{H}_{i} 
\backslash
langle X 
\backslash
rangle 
\backslash
arrow[r] & 
\backslash
mathrm{H}_{i} 
\backslash
langle Y 
\backslash
rangle 
\backslash
arrow[r] 
\backslash
arrow[d, phantom, ""{coordinate, name=Z}] & 
\backslash
mathrm{H}_{i} 
\backslash
langle Z 
\backslash
rangle 
\backslash
arrow[dll,  swap, rounded corners, to path={ -- ([xshift=2ex]
\backslash
tikztostart.east) |- (Z) [near end]
\backslash
tikztonodes -| ([xshift=-2ex]
\backslash
tikztotarget.west) -- (
\backslash
tikztotarget)}] 
\backslash

\backslash
 & 
\backslash
