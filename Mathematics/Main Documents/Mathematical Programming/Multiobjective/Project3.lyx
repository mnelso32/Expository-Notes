\text{subject to} & \quad e_{1}\geq0\nonumber \\
 & \quad e_{2}\leq0\nonumber \\
 & \quad e_{3}\leq0\label{eq:mybop}\\
 & \quad S\leq0\nonumber \\
 & \quad s\geq0\nonumber \\
 & \quad0\leqq\boldsymbol{x}\leqq2\nonumber 
\end{align}

\end_inset

