\begin_inset Formula 
\begin{equation}
[\cdot]_{\mu,h}=\mu(h\otimes1-1\otimes h),\quad\text{and}\quad[\cdot]_{h,\mu_{h}}=h(\mu_{h}\otimes1-1\otimes\mu_{h}).\label{eq:additionalsigns-1}
